<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Styles/Fonts.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <BitmapImage x:Key="icon" UriSource="/Resources/Images/icon.png"/>
    <BitmapImage x:Key="ic_settings_light" UriSource="/Resources/Images/ic_settings-light.png"/>
    <BitmapImage x:Key="ic_settings_dark" UriSource="/Resources/Images/ic_settings-dark.png"/>
    <BitmapImage x:Key="ic_dashboard" UriSource="/Resources/Images/ic_dashboard.png"/>
    <BitmapImage x:Key="ic_maintenance" UriSource="/Resources/Images/ic_maintenance.png"/>
    <BitmapImage x:Key="ic_reports" UriSource="/Resources/Images/ic_reports.png"/>
    <BitmapImage x:Key="ic_pos" UriSource="/Resources/Images/ic_pos.png"/>
    <BitmapImage x:Key="ic_inventory" UriSource="/Resources/Images/ic_inventory.png"/>
    <BitmapImage x:Key="ic_settings" UriSource="/Resources/Images/ic_settings.png"/>

    <Style x:Key="TextBlockFabricIcons" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="{StaticResource FabricIcons}" />
        <Setter Property="FontSize" Value="14" />
    </Style>
</ResourceDictionary>